{
  "name": "frontend-template",
  "version": "0.0.0",
  "scripts": {
    "e2e": "ng e2e",
    "e2e:test": "npx nightwatch ./nightwatch",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "init": "ng g @moleculemaker/ui-lib:deployment && npm run openapi:live",
    "openapi:clean": "rm -rf src/app/api/",
    "openapi-mmli-backend": "openapi-generator-cli generate --skip-validate-spec -i openapi/mmli-backend.openapi.json -g typescript-angular -o src/app/api/mmli-backend/v1",
    "openapi-mmli-backend:sync": "curl https://mmli.fastapi.staging.mmli1.ncsa.illinois.edu/openapi.json > openapi/mmli-backend.openapi.json && cat openapi/mmli-backend.openapi.json",
    "openapi-mmli-backend:live": "npm run openapi-mmli-backend:sync && npm run openapi",
    "openapi-mmli-backend:sync:local": "curl http://localhost:8080/openapi.json > openapi/mmli-backend.openapi.json && cat openapi/mmli-backend.openapi.json",
    "openapi-mmli-backend:live:local": "npm run openapi-mmli-backend:sync:local && npm run openapi",
    "openapi-cleandb": "openapi-generator-cli generate --skip-validate-spec -i openapi/cleandb.openapi.json -g typescript-angular -o src/app/api/cleandb/v1",
    "openapi-cleandb:sync": "curl https://cleandb-api.fastapi.staging.mmli1.ncsa.illinois.edu/api/v1/openapi.json > openapi/cleandb.openapi.json && cat openapi/cleandb.openapi.json",
    "openapi-cleandb:live": "npm run openapi-cleandb:sync && npm run openapi",
    "openapi-cleandb:sync:local": "curl http://localhost:8000/api/v1/openapi.json > openapi/cleandb.openapi.json && cat openapi/cleandb.openapi.json",
    "openapi-cleandb:live:local": "npm run openapi-cleandb:sync:local && npm run openapi-cleandb",
    "storybook": "ng run test:storybook",
    "build-storybook": "ng run test:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.7",
    "@angular/common": "^18.2.7",
    "@angular/compiler": "^18.2.7",
    "@angular/core": "^18.2.7",
    "@angular/forms": "^18.2.7",
    "@angular/platform-browser": "^18.2.7",
    "@angular/platform-browser-dynamic": "^18.2.7",
    "@angular/router": "^18.2.7",
    "@chemaxon/marvinjs": "^23.11.0",
    "chart.js": "^3",
    "ngx-matomo-client": "^6.3.1",
    "nightwatch": "^3.8.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.11",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.7",
    "@chromatic-com/storybook": "^1.9.0",
    "@compodoc/compodoc": "^1.1.25",
    "@moleculemaker/dev-tool": "^0.1.3",
    "@nightwatch/schematics": "^1.2.1",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@storybook/addon-docs": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/angular": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@swc/core": "^1.7.40",
    "@types/d3": "^7.4.3",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.0",
    "@types/nightwatch": "2.3.32",
    "@types/node": "^18.15.3",
    "@types/uuid": "^10.0.0",
    "chromedriver": "130.0.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nightwatch": "3.8.1",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  }
}
